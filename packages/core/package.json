{
  "name": "@quantulabs/hivemind-core",
  "version": "0.1.0",
  "description": "Core providers and consensus logic for Hivemind multi-model AI",
  "license": "MIT",
  "author": "QuantuLabs",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantuLabs/Hivemind.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/QuantuLabs/Hivemind#readme",
  "keywords": ["ai", "llm", "openai", "anthropic", "gemini", "consensus"],
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  }
}
